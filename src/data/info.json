{
    "nodeType": "t3.medium",
    "nodesMin": 3,
    "nodesMax": 4,
    "clusterName": "eks-cluster05",
    "AWSCredentials": {
        "aws_access_key_id": "",
        "aws_secret_access_key": "",
        "aws_default_region": "us-east-1"
    },
    "ingress": {
        "install": {
            "ingressNginxUrl": "https://kubernetes.github.io/ingress-nginx",
            "name": "ingress-nginx",
            "controller": "LoadBalancer",
            "namespace": "ingress-nginx/ingress-nginx",
            "version": "4.0.18",
            "serviceName": "ingress-nginx-controller"
        }
    },
    "rancher": {
        "install": {
            "rancherServerUrl": "https://releases.rancher.com/server-charts/latest",
            "name": "rancher-latest",
            "controller": "LoadBalancer",
            "namespace": "cattle-system",
            "serviceName": "rancher-latest/rancher",
            "jetstack": "jetstack",
            "jetstackUrl": "https://charts.jetstack.io",
            "serverUrl": "rancher.my.org",
            "serverPassword": "admin"
        },
        "certManager": {
            "url": "https://github.com/cert-manager/cert-manager/releases/download/v1.7.1/cert-manager.crds.yaml",
            "namespace": "cert-manager",
            "serviceName": "jetstack/cert-manager",
            "version": "v1.7.1"
        }
    }
}